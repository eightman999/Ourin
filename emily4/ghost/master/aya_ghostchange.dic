//-----ゴースト名変換処理------------------------------------------------------------
GhostNameConvert
{
	_gname = _argv[0]
	
	//せりこ
	if ASEARCH(_gname,'せりこXP','せりこDP') >= 0 {
		'せりこ'
		return
	}
	//さくら
	if ASEARCH(_gname,'．さくら','偽補完さくら') >= 0 {
		'さくら'
		return
	}
	//床さくら
	if ASEARCH(_gname,'机さくら','椅子さくら','炬燵さくら') >= 0 {
		'床さくら'
		return
	}
	//エミリ
	if _gname == 'えみりぃ' {
		'エミリ'
		return
	}
	//明らかにでべろぱトークな人のみ
	if ASEARCH(_gname,'みさき','さとー','おとーさん','ゆねね～','あれ','とじぇたん','アルフェイル','いおにゃん','夜霧たん') >= 0 {
		'でべろぱ'
		return
	}
	
	''
}

//-----魔術師さんむけ呼び出し------------------------------------------------------------
GhostCall_Magic
{
	"\u\s[10]\h\s[5]%(charname(0))、\w5%(_argv[0])に対抗して、\w5炎のまほーいっきまーす！\w9\n\s[24]うむむむむむ\w5…\w5…\w9\w9\u\s[12]%(charname(0))、\w5無茶したら駄目だっていつも言ってるの\w9\_s[0,1]\s[-1]\n\n（ぼむっ！）\_s\_w[1500]\h\s[40]\u\s[14]\n\n…\w5…\w5無茶するな　\w9下手な魔法は　\w9怪我のもと。\w9\w9\h\n\nぇぅ～。\w5いつもこのオチなんて\w5…\w5…。"
}

//-----標準反応------------------------------------------------------------
OnGhostChangeCallNormal
{{START_CHANGE
	when EMS_GC_CLOSED {
		_name = E.AddKeisyou(reference[0],'さん')
		
		if RE_SEARCH(change_plain_talk,'えんい[ー～]') {
			"\u\s[10]\h\s[5]%(_name)、\w5えんい～。\w9\w9\uそのあいさつ、\w5ちょっと変な気もするけど。\w9\w9\h\s[27]\n\nえ？\w5ゴーストさんたちの一般的なあいさつって聞いたんだけど。\w9\w9\u\n\n\s[11]…\w5…\w5まぁ、\w5間違いじゃないけどね。\e"
		}
		else {
			"\u\s[10]\h\s[5]%(_name)、\w5おつかれさま～。\e"
		}
	}
}}END_CHANGE


