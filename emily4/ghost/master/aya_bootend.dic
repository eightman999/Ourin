//---- OnCloseイベント ------------------------------------------------------------------
OnClose
{
	if sender != basewarename {
		"\u\s[10]\h\s[3]ごめんね～。\w9\n%(username)から言われるまで、\w5帰れないんだ。"
	}
	else {
		"\u\s[10]\h\s[5]またねー。\w9\w9\-"
	}
}

//**** シェル切り替え処理 **********************************************************

//---- シェルチェンジ開始 ---------------------------------------------------------------
OnShellChanging
{
	if reference[0] == "ちいさなエミリ" {
		//伝統のぱんつえみり特殊反応！
		"\u\s[10]\h\s[21]ちょっと恥ずかしい格好だけど、\w5着替えてくるよ。\w9\w9\uぱんつ丸見え。\w9\w9\h\s[4]\n\nそれは言わないで。\e"
	}
	elseif "[新]" _in_ reference[0] {
		""
	}
	elseif "[旧]" _in_ reference[0] {
		""
	}
	else {
		"\u\s[10]\h\s[5]おきがえー。\e"
	}
}

//---- シェルチェンジ完了 ---------------------------------------------------------------
OnShellChanged
{
	bootinit = 0
	
	if reference[0] == "ちいさなエミリ" {
		//伝統のぱんつえみり特殊反応！
		"\u\s[10]\h\s[21]あのさ、\w5まあさ、\w5とにかくさ。\w9\w9\u…\w5…\w5？\w9\w9\h\n\nそれなりの年齢になったから、\w5やっぱりこの格好は恥ずかしいんだけどね。\w9\w9\u\s[11]\n\n…\w5…\w5体だけしか成長してないくせに。\w9\w9\h\s[4]\n\nどういう意味それ～。\e"
	}
	elseif "[新]" _in_ reference[0] {
		"\u\s[10]\h\s[5]新しいボク！\w9いろいろ成長しちゃった！\w9\w9\u\s[11]でも精神的にはそのまんま。\w9\w9\h\s[9]\n\nううっ\w5…\w5…"
	}
	elseif "[旧]" _in_ reference[0] {
		"\u\s[10]\h\s[5]以前のボクに戻ったよ。"
	}
	else {
		"\u\s[10]\h\s[5]%(reference[0])におきがえ終了～。\w9\w9\u早かったね。\e"
	}
}


