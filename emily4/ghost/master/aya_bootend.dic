//--------------- 年始 -------------------------------------------------------
E.DaysInYear
{
	_t = GETTIME[0]
	if (_t % 4) == 0 {
		if (_t % 100) == 0 {
			if (_t % 400) == 0 {
				366
			}
			else {
				365
			}
		}
		else {
			366
		}
	}
	else {
		365
	}
}

